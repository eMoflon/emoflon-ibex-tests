#using ExtType2Doc_SCDerived.*
#using AttrCondDefLibrary.*

#rule SubPackage2DocCont #with ExtType2Doc_SCDerived

#source { 
	p:Package {
		++-subPackages->sp
	}
	
	++sp:Package
}

#target {
	dc:DocContainer
}

#correspondence {
	p2dc:Package2DocCont {
		#src->p
		#trg->dc
	}
	
	++sp2dc:Package2DocCont {
		#src->sp
		#trg->dc
	}
}

#attributeConditions {
	concat(".", p.fQname, sp.name, #derived sp.fQname)
}
