#using ExtType2Doc_SCDerived.*
#using AttrCondDefLibrary.*

#rule SubPackage2Folder #with ExtType2Doc_SCDerived

#source { 
	p:Package {
		++-subPackages->sp
	}
	
	++sp:Package
}

#target {
	f:Folder
}

#correspondence {
	p2f:Package2Folder {
		#src->p
		#trg->f
	}
	
	++sp2sf:Package2Folder {
		#src->sp
		#trg->f
	}
}

#attributeConditions {
	concat(".", p.fQname, sp.name, #derived sp.fQname)
}
