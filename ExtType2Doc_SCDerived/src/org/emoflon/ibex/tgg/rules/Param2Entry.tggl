#using ExtType2Doc_SCDerived.*
#using AttrCondDefLibrary.*

#rule Param2Entry #with ExtType2Doc_SCDerived

#source { 
	m:Method {
		++-params->p
	}
	
	++p:Parameter
}

#target {
	e:Entry
}

#correspondence {
	m2e:Method2Entry {
		#src->m
		#trg->e
	}
	
	++p2e:Param2Entry {
		#src->p
		#trg->e
	}
}

#attributeConditions {
	
}
