#using AdeleToAadl.*
#using AttrCondDefLibrary.*

#rule ruleFeatGroupAccess2FeatGroupAccessTyped #with AdeleToAadl

#source { 
	adeleType:FeatureGroup{
		++ -features ->adeleAccess

	}
	++ adeleAccess:Access
	{
		++ -classifier->adeleClassifier
	}
	adeleClassifier:Component
}

#target {
	aadlType:FeatureGroupType{
		++ -ownedFeature->aadlAccess
	}
	++ aadlAccess:Access{
		++ -featClassifierDst->aadlClassifier
}
aadlClassifier:DataSubcomponentType
}
#correspondence {
	corrComponentClassifier:CorrComponentDST{
		#src->adeleClassifier
		#trg->aadlClassifier
	}
	corrType:CorrComponentTypeFG{
		#src->adeleType
		#trg->aadlType

}
++corrAccess:CorrAccess{
	#src->adeleAccess
	#trg->aadlAccess

}

}

#attributeConditions {

}