#using AdeleToAadl.*
#using AttrCondDefLibrary.*

#rule rulePackage2AadlPackage #with AdeleToAadl

#source { 
	++adelePackage:Adele.Package
	
}

#target {
	++aadlPackage:Aadl.AdlPackage
	{
		++ -ownedPublicSection->aadlPublicPackageSection
	}
	++aadlPublicPackageSection:Aadl.PublicPackageSection 
}

#correspondence {
	++corrAxiom:CorrAxiomAP
	{
		#src->adelePackage
		#trg->aadlPackage
	}
}

#attributeConditions {
	
}
