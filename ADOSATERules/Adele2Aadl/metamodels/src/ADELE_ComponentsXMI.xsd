    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="Processor" type="ADELE_Components:Processor"/>
  <xsd:complexType name="Subprogram">
    <xsd:complexContent>
      <xsd:extension base="ADELE_Components:Component"/>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="Subprogram" type="ADELE_Components:Subprogram"/>
  <xsd:complexType name="System">
    <xsd:complexContent>
      <xsd:extension base="ADELE_Components:Component"/>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="System" type="ADELE_Components:System"/>
  <xsd:complexType name="Thread">
    <xsd:complexContent>
      <xsd:extension base="ADELE_Components:Component"/>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="Thread" type="ADELE_Components:Thread"/>
  <xsd:complexType name="ThreadGroup">
