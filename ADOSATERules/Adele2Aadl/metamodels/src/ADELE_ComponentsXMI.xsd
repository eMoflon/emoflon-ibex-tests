  </xsd:complexType>
  <xsd:element name="Package" type="ADELE_Components:Package"/>
  <xsd:complexType name="Data">
    <xsd:complexContent>
      <xsd:extension base="ADELE_Components:Component"/>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="Data" type="ADELE_Components:Data"/>
  <xsd:complexType name="Device">
    <xsd:complexContent>
      <xsd:extension base="ADELE_Components:Component"/>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="Device" type="ADELE_Components:Device"/>
  <xsd:complexType name="Memory">
    <xsd:complexContent>
