  <xsd:complexType name="Abstract">
    <xsd:complexContent>
      <xsd:extension base="ADELE_Components:Component"/>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="Abstract" type="ADELE_Components:Abstract"/>
  <xsd:complexType name="VirtualProcessor">
    <xsd:complexContent>
      <xsd:extension base="ADELE_Components:Component"/>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="VirtualProcessor" type="ADELE_Components:VirtualProcessor"/>
  <xsd:complexType name="VirtualBus">
    <xsd:complexContent>
      <xsd:extension base="ADELE_Components:Component"/>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="VirtualBus" type="ADELE_Components:VirtualBus"/>
</xsd:schema>
