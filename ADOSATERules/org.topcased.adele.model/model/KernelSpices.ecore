    <eStructuralFeatures xsi:type="ecore:EReference" name="children" upperBound="-1"
        eType="#//SKHierarchicalObject" containment="true" eOpposite="#//SKHierarchicalObject/parent"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="odSystems" upperBound="-1"
        eType="ecore:EClass ObjectDescriptionModel.ecore#//SKODSystem" containment="true"/>
  </eClassifiers>
