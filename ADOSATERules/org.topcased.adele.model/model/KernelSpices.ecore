        eType="#//SKFeatureVisibility"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="component" eType="#//SKComponent"
        eOpposite="#//SKComponent/features"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="SKFeatureVisibility">
    <eLiterals name="Provided"/>
    <eLiterals name="Internal" value="1" literal="Internal"/>
    <eLiterals name="Required" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SKRelation" eSuperTypes="#//SKHierarchicalObject">
    <eStructuralFeatures xsi:type="ecore:EReference" name="objects" lowerBound="2"
        upperBound="2" eType="#//SKObject"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SKProperty" eSuperTypes="#//SKHierarchicalObject">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EJavaObject"/>
  </eClassifiers>
</ecore:EPackage>
