#using AlgorithmToStrategy_MA.*
#using AttrCondDefLibrary.*

#rule AlgoContainerToStrategyContainer #with AlgorithmToStrategy_MA

#source { 
	++ac:AlgorithmContainer
}

#target {
	++sc:StrategyContainer
}

#correspondence {
	++ac2sc:AlgorithmsToStrategiesContainerCorr{
		#src->ac
		#trg->sc
	}
}

#attributeConditions {
	eq_string(ac.name, sc.name)
}
