<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<xsd:schema xmlns:URIFragment="platform:/resource/URIFragment/model/URIFragment.ecore" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="platform:/resource/URIFragment/model/URIFragment.ecore">
  <xsd:import namespace="http://www.omg.org/XMI" schemaLocation="platform:/plugin/org.eclipse.emf.ecore/model/XMI.xsd"/>
  <xsd:complexType name="Tree">
    <xsd:complexContent>
      <xsd:extension base="URIFragment:NamedNode">
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
          <xsd:element name="branches" type="URIFragment:Branch"/>
        </xsd:choice>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="Tree" type="URIFragment:Tree"/>
  <xsd:complexType name="Branch">
    <xsd:complexContent>
      <xsd:extension base="URIFragment:NamedNode">
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
          <xsd:element name="tree" type="URIFragment:Tree"/>
          <xsd:element name="leaves" type="URIFragment:Leaf"/>
          <xsd:element name="subBranches" type="URIFragment:Branch"/>
          <xsd:element name="parent" type="URIFragment:Branch"/>
          <xsd:element name="apples" type="URIFragment:Apple"/>
        </xsd:choice>
        <xsd:attribute name="tree" type="xsd:string"/>
        <xsd:attribute name="parent" type="xsd:string"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="Branch" type="URIFragment:Branch"/>
  <xsd:complexType name="Leaf">
    <xsd:complexContent>
      <xsd:extension base="URIFragment:NamedNode">
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
          <xsd:element name="branch" type="URIFragment:Branch"/>
        </xsd:choice>
        <xsd:attribute name="color" type="xsd:string"/>
        <xsd:attribute name="branch" type="xsd:string"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="Leaf" type="URIFragment:Leaf"/>
  <xsd:complexType name="Apple">
    <xsd:complexContent>
      <xsd:extension base="URIFragment:NamedNode">
        <xsd:choice maxOccurs="unbounded" minOccurs="0">
          <xsd:element name="branch" type="URIFragment:Branch"/>
        </xsd:choice>
        <xsd:attribute name="appleID" type="xsd:string"/>
        <xsd:attribute name="branch" type="xsd:string"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="Apple" type="URIFragment:Apple"/>
  <xsd:complexType abstract="true" name="NamedNode">
    <xsd:choice maxOccurs="unbounded" minOccurs="0">
      <xsd:element ref="xmi:Extension"/>
    </xsd:choice>
    <xsd:attribute ref="xmi:id"/>
    <xsd:attributeGroup ref="xmi:ObjectAttribs"/>
    <xsd:attribute name="name" type="xsd:string"/>
  </xsd:complexType>
  <xsd:element name="NamedNode" type="URIFragment:NamedNode"/>
</xsd:schema>
