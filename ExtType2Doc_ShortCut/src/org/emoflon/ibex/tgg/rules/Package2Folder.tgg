#using ExtType2Doc_ShortCut.*
#using AttrCondDefLibrary.*

#rule Package2Folder #with ExtType2Doc_ShortCut

#source { 
	pr:Project {
		++-rootPackages->p
	}
	
	++p:Package
}

#target {
	dc:DocContainer {
		++-folders->f
	}
	
	++f:Folder
}

#correspondence {
	pr2dc:Project2DocContainer {
		#src->pr
		#trg->dc
	}
	
	++p2f:Package2Folder {
		#src->p
		#trg->f
	}
}

#attributeConditions {
	
}
