#using ExtType2Doc_ShortCut.*
#using AttrCondDefLibrary.*

#rule SubPackage2Folder #with ExtType2Doc_ShortCut

#source { 
	p:Package {
		++-subPackages->sp
	}
	
	++sp:Package
}

#target {
	f:Folder
}

#correspondence {
	p2dc:Package2Folder {
		#src->p
		#trg->f
	}
	
	++sp2sf:Package2Folder {
		#src->sp
		#trg->f
	}
}

#attributeConditions {
	
}
