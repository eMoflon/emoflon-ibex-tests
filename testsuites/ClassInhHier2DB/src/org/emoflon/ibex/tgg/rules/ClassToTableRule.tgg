#using testsuite1_ClassInhHier2DB.*
#using AttrCondDefLibrary.*

#rule ClassToTableRule #with testsuite1_ClassInhHier2DB


#source {
	classpackage:ClassPackage
	++clazz:Clazz {
		++ - package->classpackage
	} 
}

#target {
	db:DB {
		++ - tables->table
	}
	++table:Table
}

#correspondence {
	packageToDatabase:PackageToDatabase {
		#src->classpackage
		#trg->db
	}
	++ clazzToTable:ClazzToTable {
		#src->clazz
		#trg->table
	}
}

#attributeConditions {
	eq_string(clazz.name, table.heading)
}
