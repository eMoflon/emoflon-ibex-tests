#using testsuite1_familiestopersons.*
#using AttrCondDefLibrary.*

#rule NewFamilyDaughterToFemale #extends FamilyMemberToPerson #with testsuite1_familiestopersons

#source { 
	++ f:Family {
		++ -daughters->fm
	}
	
	++ fm:FamilyMember {
		name := "daughter"
	}
}

#target {	
	++ p:Female {
		name := "daughter"
	}
}

#correspondence {
}

#attributeConditions {
}
