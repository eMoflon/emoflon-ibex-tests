#using testsuite1_familiestopersons.*
#using AttrCondDefLibrary.*

#rule MotherToFemale #extends FamilyMemberToPerson, ExistingFamilyToPerson #with testsuite1_familiestopersons

#source {
	f:Family {
		++ -mother->fm
	}
	
	++ fm:FamilyMember {
		name := "mother"
	}
}

#target {
	++ p:Female {
		name := "mother"
	}
}

#correspondence {
}

#attributeConditions {
}
