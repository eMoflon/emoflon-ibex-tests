#using testsuite1_familiestopersons.*
#using AttrCondDefLibrary.*

#rule SonToMale #extends FamilyMemberToPerson, ExistingFamilyToPerson #with testsuite1_familiestopersons

#source { 
	f:Family {
		++ -sons->fm
	}
	
	++ fm:FamilyMember {
		name := "son"
	}
}

#target {
	++ p:Male {
		name := "son"
	}
}

#correspondence {
}

#attributeConditions {
}
