#using testsuite1_familiestopersons.*
#using AttrCondDefLibrary.*

#abstract #rule FamilyMemberToPerson #with testsuite1_familiestopersons

#source { 
	fr:FamilyRegister {
		++ -families->f
	}
	
	++ f:Family
	
	++ fm:FamilyMember
}

#target {
	pr:PersonRegister {
		++ -persons->p
	}
	
	++ p:Person
}

#correspondence {
	roots:RegisterToRegister {
		#src->fr
		#trg->pr
	}
	
	++ memberToPerson:FamilyMemberToPerson {
		#src->fm
		#trg->p
	}
}

#attributeConditions {
}
