#using testsuite1_familiestopersons.*
#using AttrCondDefLibrary.*

#rule FatherToMale #extends FamilyMemberToPerson, ExistingFamilyToPerson #with testsuite1_familiestopersons

#source { 
	f:Family {
		++ -father->fm
	}
	
	++ fm:FamilyMember {
		name := "father"
	}
}

#target {
	++ p:Male {
		name := "father"
	}
}

#correspondence {	
}

#attributeConditions {
}
