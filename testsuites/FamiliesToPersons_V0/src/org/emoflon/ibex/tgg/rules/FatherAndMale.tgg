#using testsuite1_familiestopersons.*
#using AttrCondDefLibrary.*

#rule FatherAndMale #with testsuite1_familiestopersons

#source { 
	fr:FamilyRegister {
		-families->f
	}
	
	f:Family {
		++ -father->father
	}
	
	++ father:FamilyMember {
		name := "father"
	}
}

#target {
	pr:PersonRegister {
		++ -persons->p
	}
	
	++ p:Male {
		name := "father"
	}
}

#correspondence {
	
}

#attributeConditions {
}
