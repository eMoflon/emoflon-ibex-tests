#using testsuite1_familiestopersons.*
#using AttrCondDefLibrary.*

#rule FatherToNothing #with testsuite1_familiestopersons

#source { 
	fr:FamilyRegister {
		-families->f
	}
	
	f:Family {
		++ -father->father
	}
	
	++ father:FamilyMember {
		name := "father"
	}
}

#target {
	pr:PersonRegister
}

#correspondence {
	roots:RegisterToRegister {
		#src->fr
		#trg->pr
	}
}

#attributeConditions {
}
