#using FamiliesToPersons_V1.*
#using AttrCondDefLibrary.*

#rule CreateDaughterInExistingFamily #with FamiliesToPersons_V1

#source {
	f:Family {
		++ -daughters->m
	}
	++ m:FamilyMember
}

#target {
	preg:PersonRegister {
		++-persons->p
	}
	
	++ p:Female
}

#correspondence {
	++ p2m:FamilyMemberToPerson {
		#src->m
		#trg->p
	}
}

#attributeConditions {
	eq_string(m.name, p.name)
}
