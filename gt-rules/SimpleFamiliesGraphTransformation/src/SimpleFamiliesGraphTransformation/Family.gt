import "platform:/resource/SimpleFamilies/model/SimpleFamilies.ecore"

rule createFamily {
	register: FamilyRegister {
		++ -families -> family
	}

	++ family: Family
}

rule deleteFamily {
	register: FamilyRegister {
		-- -families -> family
	}

	-- family: Family
}

rule findFamily {
	register: FamilyRegister {
		-families -> family
	}

	family: Family
}
