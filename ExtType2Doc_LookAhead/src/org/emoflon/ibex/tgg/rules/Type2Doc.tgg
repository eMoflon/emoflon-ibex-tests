#using ExtType2Doc_LookAhead.*
#using AttrCondDefLibrary.*

#rule Type2Doc #with ExtType2Doc_LookAhead

#source { 
	p:Package {
		++-types->t
	}
	
	++t:Type
}

#target {
	f:Folder {
		++-docs->d
	}
	
	++d:Doc
}

#correspondence {
	p2f:Package2Folder {
		#src->p
		#trg->f
	}
	
	++t2d:Type2Doc {
		#src->t
		#trg->d
	}
}

#attributeConditions {
	eq_string(t.name, d.name)
}
