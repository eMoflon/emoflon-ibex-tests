rule Field2Entry {
	
	source { 
		[=] t:T.Type {
			[+] -fields->f
		}
		
		[+] f:T.Field
	}
	
	target {
		[=] d:D.Doc {
			[+] -entries->et
		}
		
		[+] et:D.Entry {
			.type:=enums.FIELD;
		}
	}
	
	correspondence {
		[=] t2d:Type2Doc {
			src->t
			trg->d
		}
		
		[+] f2e:Field2Entry {
			src->f
			trg->et
		}
	}
	
	attributeConditions {
		DefaultAttributeConditionLibrary.eq_string(f.name, et.name)
	}

}

