#import "platform:/resource/Ar/model/Ar.ecore"
#import "platform:/resource/Qou/model/Qou.ecore"

#schema ArToQou
	
#source {
	Ar
}

#target { 
	Qou
} 

#correspondence {
	R2Q {
		#src->RClass
		#trg->QClass
	}
	
	A2X {
		#src->AClass
		#trg->XClass
	}
	
	B2X {
		#src->BClass
		#trg->XClass
	}
}
