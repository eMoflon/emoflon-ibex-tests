#using ArToQou.*
#using AttrCondDefLibrary.*

#rule SubA2X #with ArToQou

#source { 
	b:BClass {
		++-nextA->a
	}
	++a:AClass
}

#target {
	q:QClass {
		-exs->superX
		++-exs->subX
	}
	superX:XClass
	++subX:XClass
}

#correspondence {
	b2x:B2X {
		#src->b
		#trg->superX
	}
	++a2x:A2X {
		#src->a
		#trg->subX
	}
}

#attributeConditions {
	eq_string(a.name, subX.name)
}
