#using ExtType2Doc_ConcSync.*
#using AttrCondDefLibrary.*

#rule Field2Entry #with ExtType2Doc_ConcSync

#source { 
	t:Type {
		++-fields->f
	}
	
	++f:Field
}

#target {
	d:Doc {
		++-entries->e
	}
	
	++e:Entry {
		type:=enum::EntryType::FIELD
	}
}

#correspondence {
	t2d:Type2Doc {
		#src->t
		#trg->d
	}
	
	++f2e:Field2Entry {
		#src->f
		#trg->e
	}
}

#attributeConditions {
	eq_string(f.name, e.name)
}
