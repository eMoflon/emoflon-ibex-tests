#using Express2UML.*
#using AttrCondDefLibrary.*

#rule RootPackage2SchemaContainer #with Express2UML

#source {
	++s:Schema

	++sc:SchemaContainer {
		++-schemas -> s
	}
}

#target {
	++p:Package
}

#correspondence {
	++sc2rp: SchemaContainer2RootPackage {
		#src->sc
		#trg->p
	}
	
	++s2p: Schema2Package {
		#src->s
		#trg->p
	}
}

#attributeConditions {
	eq_string(s.name, p.name)
}


