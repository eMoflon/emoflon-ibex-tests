#using Express2UML.*
#using AttrCondDefLibrary.*

#rule IntegerAttribute2Integer #with Express2UML

#source {
	++ia:IntegerAttribute {
		optional := false
	}

	e:Entity {
		abstracct == false
		++ -attributes -> ia
	}
}

#target {
	++i:Integer

	c:Class {
		++ -attributes -> i
	}
}

#correspondence {
	e2c:Entity2Class {
		#src->e
		#trg->c
	}

	++ it2i:IntegerTypeAttribute2Integer {
		#src->ia
		#trg->i
	}
}

#attributeConditions {
	eq_string(ia.name, i.name)
}


