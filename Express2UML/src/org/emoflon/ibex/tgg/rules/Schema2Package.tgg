#using Express2UML.*
#using AttrCondDefLibrary.*

#rule Schema2Package #with Express2UML

#source {
	++s:Schema

	sc:SchemaContainer {
		++ -schemas -> s
	}
}

#target {
	++sp:Package

	pp:Package {
		++- subpackages -> sp
	}
}

#correspondence {
	++s2sp:Schema2Package {
		#src->s
		#trg->sp
	}
}

#attributeConditions {
	eq_string(s.name, sp.name)
}


