#using FamiliesToPersons_MA.*
#using AttrCondDefLibrary.*

#complement Daughter2Female #for CreateFamily

#source { 
	family : Family{
		++ -daughters-> daughter
	}
	
	++ daughter : FamilyMember
	
}

#target {
	
	personReg : PersonRegister {
		++ -persons->femalePerson
	}
	
	++ femalePerson : Female
}

#correspondence {
	++ daughter2femalePerson : Families2Persons_MA.FamilyMember2Person {
		#src->daughter
		#trg->femalePerson
	}
}

#attributeConditions {
	concat(", ", daughter.name, family.name, femalePerson.name)
}
