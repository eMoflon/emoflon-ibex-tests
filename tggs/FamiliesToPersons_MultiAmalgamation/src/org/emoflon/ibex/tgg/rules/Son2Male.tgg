#using FamiliesToPersons_MA.*
#using AttrCondDefLibrary.*

#complement Son2Male #for CreateFamily

#source { 
	
	family : Family {
		++ -sons->son
	}
	
	++ son : FamilyMember
}

#target {
	personReg : PersonRegister{
		++ -persons-> malePerson
	}
	++ malePerson : Male
}

#correspondence {
	++ son2malePerson : Families2Persons_MA.FamilyMember2Person{
		#src->son
		#trg->malePerson
	}
}

#attributeConditions {
	concat(", ", son.name, family.name, malePerson.name)
}
