#using Class2TestClass_MA.*
#using AttrCondDefLibrary.*

#complement CreateTestClass #for CreateClass

#source { 	
	 class : Clazz
}

#target {
	
	testSuite : DB{
		++ -tables->testClass
	}
	
	++ testClass : Table

}

#correspondence {
	++ class2testClass : ClazzToTestClass {
		#src->class
		#trg->testClass
	}
}

#attributeConditions {
	eq_string(class.name, testClass.heading)
}
