#using FamiliesToPersons_V1.*
#using AttrCondDefLibrary.*

#rule HandleRegisters #with FamiliesToPersons_V1

#source { 
	++ fr:FamilyRegister
}

#target {
	++ pr:PersonRegister
} 

#correspondence {
	++ roots:RegisterToRegister {
		#src->fr
		#trg->pr
	}
}

#attributeConditions {
}

#nac NoDuplicatedFamilyRegister #for HandleRegisters

#source {
	other:FamilyRegister
}

#nac NoDuplicatedpersonRegister #for HandleRegisters

#target {
	other:PersonRegister
}

