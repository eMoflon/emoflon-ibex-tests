#rule CreateFamily #with FamiliesToPersons_MA

#source { 
	familyReg : FamilyRegister{
		++-families -> family
	}
	
	++ family : Family
}

#target {
	
	personReg : PersonRegister
}

#correspondence {
	 familyReg2personReg : FamiliesReg2PersonsRegCorr {
	 	#src->familyReg
	 	#trg->personReg
	 }
}

