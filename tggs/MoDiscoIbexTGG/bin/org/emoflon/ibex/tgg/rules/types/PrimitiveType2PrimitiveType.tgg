#using MoDiscoIbexTGG.*
#using AttrCondDefLibrary.*

#abstract #rule PrimitiveType2PrimitiveType #with MoDiscoIbexTGG

#source { 
	jm : Model{
		++-orphanTypes->jpt
	}
	
	++jpt : PrimitiveType
}

#target {
	up : Package{
		++-packagedElement->umpt
	}
	
	++ umpt : PrimitiveType
}

#correspondence {
	jm2up:JavaModel2UMLPackage{
		#src->jm
		#trg->up
	}
	++jpt2upt2:JavaType2UMLType{
		#src->jpt
		#trg->umpt
	}
}

#attributeConditions {
	eq_string(jpt.name, umpt.name)
}


#rule PrimitiveTypeBoolean #extends PrimitiveType2PrimitiveType #with MoDiscoIbexTGG

#source { 

	++jpt : PrimitiveTypeBoolean
}

