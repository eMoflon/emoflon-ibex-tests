#using ClassInhHier2DB_MA.*
#using AttrCondDefLibrary.*

#rule PackageToDatabaseRule #with ClassInhHier2DB_MA

#source { 
	++package : ClassPackage
	
}

#target {
	++db : DB
}

#correspondence {
	++ packageToDb: Package2DB{
		#src->package
		#trg->db
	}
}

#attributeConditions {
	eq_string(package.name, db.title)
}
