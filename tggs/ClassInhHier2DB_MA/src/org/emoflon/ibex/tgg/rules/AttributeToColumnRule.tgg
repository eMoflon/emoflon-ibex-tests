#using ClassInhHier2DB_MA.*
#using AttrCondDefLibrary.*

#rule AttributeToColumnRule #with ClassInhHier2DB_MA

#source { 
	clazz:Clazz{
		++ -attributes->attribute
	}
	
	++attribute: Attribute
} 

#target {
	table:Table{
		++ -columns->col
	}
	
	++col:Column
}

#correspondence {
	clazzToTable:Clazz2DirectTable{
		#src->clazz
		#trg->table
	}
}

#attributeConditions {
	eq_string(attribute.name, col.name)
}
