#using MocaTreeToProcess.*
#using AttrCondDefLibrary.*

#rule FilesToModule #with MocaTreeToProcess

#source { 
	folder : Folder
	++ foo : File {
		++ -folder->folder
	}
	++ baa : File {
		++ -folder->folder
	}
	
}

#target {
	sysmod : SystemModule 
	++ foobaa : Module {
		++ -systemModule->sysmod
	}
}

#correspondence {
	folder2sysmod : FolderToSystemModuleCorr {
		#src->folder
		#trg->sysmod
	}
}

