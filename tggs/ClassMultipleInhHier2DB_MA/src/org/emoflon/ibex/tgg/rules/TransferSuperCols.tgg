#using ClassMultipleInhHier2DB_MA.*
#using AttrCondDefLibrary.*

#complement TransferSuperCols #for SubClassToTable

#target {
	
	superTable:Table{
		-columns->col
	}
	
	col:Column
	
	subTable:Table{
		++ -columns->transferedCols
	}
	
	++ transferedCols:Column
	
}

#attributeConditions {
	eq_string(col.name, transferedCols.name)
}