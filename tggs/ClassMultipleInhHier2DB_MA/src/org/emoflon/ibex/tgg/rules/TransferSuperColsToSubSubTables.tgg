#using ClassMultipleInhHier2DB_MA.*
#using AttrCondDefLibrary.*

#complement TransferSuperColsToSubSubTables #for SubClassToTable

#source {
	
	subClazz:Clazz
	
}

#target {
	
	superTable:Table{
		-columns->col
	}
	
	col:Column
	
	subSubTable:Table{
		++ -columns->transferedCols
	}
	
	++ transferedCols:Column
	
}

#correspondence {
	
	SubClassToSubSubTable:Clazz2SubTable{
		#src->subClazz
		#trg->subSubTable
	}
	
	}

#attributeConditions {
	eq_string(col.name, transferedCols.name)
}