		[=] classDec : Java.AbstractTypeDeclaration {
			[+] -bodyDeclarations->method
		}
		
		[+] method : Java.MethodDeclaration{
			
		}
	}
	
	target {
	
		[+] operation : UML2.Operation{
		}
	
	}
	
	correspondence {
		
		[+] md2o : MethodDec2Operation {
			src->method
