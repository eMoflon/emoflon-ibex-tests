<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <!-- @generated simpleJava -->
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="simpleJava.presentation.SimpleJavaModelWizardID"
            name="%_UI_SimpleJavaModelWizard_label"
            class="simpleJava.presentation.SimpleJavaModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/SimpleJavaModelFile.gif">
         <description>%_UI_SimpleJavaModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <!-- @generated simpleJava -->
      <editor
            id="simpleJava.presentation.SimpleJavaEditorID"
            name="%_UI_SimpleJavaEditor_label"
            icon="icons/full/obj16/SimpleJavaModelFile.gif"
            extensions="simplejava"
            class="simpleJava.presentation.SimpleJavaEditor"
            contributorClass="simpleJava.presentation.SimpleJavaActionBarContributor">
      </editor>
   </extension>

</plugin>
