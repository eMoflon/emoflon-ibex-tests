package chemistry.gt
import "platform:/resource/SimpleChemistry/model/SimpleChemistry.ecore"

rule forbiddenCreateH20{
	[=] h1: H {
		[+] -connections -> o
	}	
	[=] h2: H{
		[+] -connections -> o
	}
	[=] o: O{
		[+] -connections -> h2
		[+] -connections -> h1
	}
};

pattern notDisjunctPattern{
	[=] h1: H
	[=] h2: H
};
