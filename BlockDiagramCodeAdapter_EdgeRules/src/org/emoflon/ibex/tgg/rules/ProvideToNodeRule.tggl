rule ProvideToNodeRule {	
	source {
		[+] provide:BlockDiagram.Provide
	}
	
	target {
		[+] id:MocaTree.Attribute {
			.name := "id";
		}
		[+] provideNode:MocaTree.Node {
			.name := "PROVIDE";
			[+] - attribute->id
		}
	}
	
	correspondence {
		[+] provideToNode :ProvideToNode {
			src->provide
			trg->provideNode
		}
	}
	
	attributeConditions {
		DefaultAttributeConditionLibrary.eq_string (provide.id, id.value)
	}
}

